{
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": 2015,
		"sourceType": "module"
	},
	"rules": {
		"linebreak-style": [
			"error",
			"windows"
		],
		"no-console": [
			"error",
			{ 
				"allow": [
					"warn",
					"error",
					"log"
				]
			}
		]
	}
}